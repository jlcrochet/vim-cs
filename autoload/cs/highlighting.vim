" Caching highlight ID's that are used for indentation
const g:cs#highlighting#comment = hlID("csComment")
const g:cs#highlighting#comment_delimiter = hlID("csCommentDelimiter")
const g:cs#highlighting#string = hlID("csString")
const g:cs#highlighting#delimiter = hlID("csDelimiter")
